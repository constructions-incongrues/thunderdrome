[
    {{ range $index, $e := (where .Data.Pages "Type" "event" ) }}
    {{ if $index }}, {{ end }}
    {
        "title": "{{ .Title }}",
        "start": "{{ dateFormat "2006-01-02" .Date }}",
        {{ with .Params.end }}
        "end": "{{- . -}}",
        {{ end }}
        "allDay": {{ .Params.allDay }},
        "url": "{{ .Permalink | absURL }}",
        "description:": {{ .Params.description | jsonify }}
    }
    {{ end }}
]